{
  "name": "@bem-react/pack",
  "version": "1.4.0",
  "description": "A tool for building and prepare components for publishing",
  "homepage": "https://github.com/bem/bem-react/tree/master/packages/pack",
  "repository": "https://github.com/bem/bem-react",
  "keywords": ["bem", "build", "react"],
  "bin": {
    "pack": "bin/pack"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": ["bin", "lib", "oclif.manifest.json"],
  "license": "MPL-2.0",
  "scripts": {
    "build:manifest": "oclif-dev manifest",
    "build": "tsc",
    "cleanup": "rm -rf lib",
    "dev": "tsc -w",
    "prepare": "npm run build:manifest",
    "prepublishOnly": "npm run cleanup && npm run build",
    "test": "exit 0"
  },
  "dependencies": {
    "@oclif/command": "1.8.0",
    "chalk": "4.1.0",
    "debug": "4.1.1",
    "fast-glob": "3.2.4",
    "fs-extra": "9.0.1",
    "log-update": "4.0.0",
    "postcss": "7.0.32"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@types/debug": "4.1.5",
    "@types/fs-extra": "9.0.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "oclif": {
    "commands": "lib/cli",
    "bin": "pack"
  }
}
